		<param
        type="text"
        name="partner_id"
        id="partner_id"
        label="Zanox.com partner_id"
        description="Enter your zanox.com partner_id"
        size="80"
		default="0"
        maxLength="255" />
			
		
			<param
        type="text"
        name="mode"
        id="mode"
        label="Mode"
        description="Select mode"
		default="1"
        size="80"
        maxLength="255" />
		
			<param
        type="text"
        name="parner_id"
        id="parner_id"
        label="Partner ID"
        description="Enter your ID"
		default=""
        size="80"
        maxLength="255" />
		
			
		
       
    
