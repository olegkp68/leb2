	<config>
	 <fields name = "params" addfieldpath="/modules/mod_quiz/fields">
	   <fieldset name="basic">
		<field
            name="cache"
            type="list"
            default="0"
            label="Joomla Caching"
            description="">
            <option value="0">Disabled</option>
			<option value="1">Enabled</option>
        </field>
		
		 </fieldset>
 
 <fieldset name="import" label="Import&amp;Export">
		<field name="uploaddata" type="ajaxuploadfield" label="Overwrite data with this import" description="Upload new XLS file" data-label="Upload new XLSX..." data-cmd="uploadxls" data-status="true" />
		<field name="downloaddata" type="ajaxfield" label="Download" description="Download Current XLSX" data-label="Download current XLSX..." data-cmd="downloaddata"  data-isdownload="true" />
		
		<field name="downloadstats" type="ajaxfield" label="Download Stats" description="Download usage stats" data-label="Download stats..." data-cmd="downloadstats"  data-isdownload="true" />
	   
		</fieldset>
		
		
	</fields>
	</config> 
	
	
